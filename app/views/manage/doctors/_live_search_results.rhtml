<table>
  <thead>
    <th>Name</th>
    <th>Alias</th>
    <th>Telephone</th>
  </thead>
<% for doctor in @results %>
  <tr>
    <td><%= doctor.friendly_name %></td>
    <td><em><%= doctor.alias %></em></td>
    <td><%= doctor.telephone %></td>
    <td><%= link_to 'Show', doctor_path(doctor) %></td>
    <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
    <td><%= link_to 'Destroy', doctor_path(doctor), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
